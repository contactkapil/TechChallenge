output "network_name" {
  value = azurerm_virtual_network.vnet01.name
  description = "Name of the Virtual network"
}

output "websubnet_id" {
  value = azurerm_subnet.gateway.id
  description = "Id of subnet to be used for gateway in the network"
}

output "websubnet_id" {
  value = azurerm_subnet.web-subnet.id
  description = "Id of compute subnet in the network"
}

output "computesubnet_id" {
  value = azurerm_subnet.compute-subnet.id
  description = "Id of compute subnet in the network"
}

output "dbsubnet_id" {
  value = azurerm_subnet.db-subnet.id
  description = "Id of dbsubnet in the network"
}

output "bastionsubnet_id" {
  value = azurerm_subnet.bastion-subnet.id
  description = "Id of bastion in the network"
}
